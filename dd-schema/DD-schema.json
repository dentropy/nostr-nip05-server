{
    "app_names": [
        "nostr-nip05-server"
    ],
    "app_version": "0.0.1",
    "tags": [
        "nostr"
    ],
    "authors": [
        "dentropy@ddaemon.org"
    ],
    "dependencies": [
        "nip05",
        "pki",
        "dd20",
        "lightning-bitcoin-to-dd20",
        "dd-rbac",
        "coupon-to-dd20",
        "domain-name-metadata"
    ],
    "schemas": [
        {
            "schema_name": "default_well_known_domain_name",
            "index_type": "NOT_LOGGED",
            "rxdb_json": {
                "version": 0,
                "primaryKey": "id",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "maxLength" : 100
